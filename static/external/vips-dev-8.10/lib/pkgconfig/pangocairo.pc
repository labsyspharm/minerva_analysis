prefix=/data/inst
libdir=${prefix}/lib
includedir=${prefix}/include

Name: Pango Cairo
Description: Cairo rendering support for Pango
Version: 1.45.3
Requires: pango, cairo >=  1.12.10
Requires.private: pangoft2, pangowin32, glib-2.0 >=  2.60, gobject-2.0 >=  2.60, gio-2.0 >=  2.60, fribidi >=  0.19.7, harfbuzz >=  2.0.0, fontconfig >=  2.11.91, freetype2
Libs: -L${libdir} -lpangocairo-1.0
Libs.private: -lm -lgdi32
Cflags: -I${includedir}/pango-1.0
