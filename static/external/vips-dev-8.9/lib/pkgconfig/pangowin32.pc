prefix=/data/inst
libdir=${prefix}/lib
includedir=${prefix}/include

Name: Pango Win32
Description: Win32 GDI font support for Pango
Version: 1.43.0
Requires: pango
Requires.private: glib-2.0 >=  2.38.0, gobject-2.0 >=  2.38.0, fribidi >=  0.19.7, harfbuzz >=  1.4.2, fontconfig >=  2.11.91, freetype2, cairo >=  1.12.10
Libs: -L${libdir} -lpangowin32-1.0
Libs.private: -lm -lusp10 -lgdi32
Cflags: -I${includedir}/pango-1.0
